apiVersion: 1

providers:
  - name: '{{ grafana_dashboards_provider }}'
    orgId: 1
    folder: '{{ grafana_dashboards_folder }}'
    type: file
    disableDeletion: false
    editable: true
    updateIntervalSeconds: 10
    allowUiUpdates: true
    options:
      path: {{ grafana_provisioning_dir }}/dashboards
      foldersFromFilesStructure: true
